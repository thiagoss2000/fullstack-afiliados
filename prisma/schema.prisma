generator client {
  provider = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transactions {
  id  Int @id @default(autoincrement())
  type  Int
  date  String
  product String
  price Int
  name  String
}